						<script type='text/javascript'>
							$(function() {
								$('#LanguageText').html($('#Lan_en').html()) ;
								$('#LanguageIcon').attr('src', 'https://cdn.the-blueprints.com/images/countries/en.svg') ;
							}) ;

							function SwitchLanguage (language) {
								$('#LanguageText').hide() ;
								$('#LanguageText').html($('#Lan_' + language).html()) ;
								$('#LanguageText').fadeIn() ;

								$('#LanguageIcon').hide() ;
								$('#LanguageIcon').attr('src', 'https://cdn.the-blueprints.com/images/countries/' + language + '.svg') ;
								$('#LanguageIcon').fadeIn() ;

								// AJAX save
								$.post("https://www.the-blueprints.com/ajax.php?language/set/" + language + "/", {}, function(data) {
									if(data.length > 0) {
										returnData = $.parseJSON(data) ;

										if(returnData.status == 'ok') {
											// Reload page
											location.reload() ;
										}
									}
								}) ;
							}

							function SwitchCurrency (currency) {
								// AJAX save
								$.post("https://www.the-blueprints.com/ajax.php?settings/currency/" + currency + "/", {}, function(data) {
									if(data.length > 0) {
										returnData = $.parseJSON(data) ;

										if(returnData.status == 'ok') {
											// Reload page
											location.reload() ;
										}
									}
								}) ;

							}
						</script>
					</div>
				</div>
